<!DOCTYPE HTML>
  <head>
    <title><%= current_page.data.title || "fivecat.gq" %></title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <!--[if lte IE 8]><script src="js/html5shiv.js"></script><![endif]-->

    <%= javascript_include_tag  "jquery.min", "skel.min", "skel-layers.min", "init" %>
		<noscript>
      <%= stylesheet_link_tag "skel", "style", "style-xlarge" %>
    </noscript>
  </head>

  <body id="top">

    <!-- Header -->
    <header id="header" class="skel-layers-fixed">
      <h1><a href="/">fivecat.gq</a></h1>
      <div id='nav'></div>
    </header>

    <!-- Translator -->
    <div id="translate" style="float: right !important">
      <div id='MicrosoftTranslatorWidget' class='Dark' style='color:white;background-color:#555555'></div>
    </div>

    <!-- Main -->
    <section id="main" class="wrapper style1">
      <div class="container">
        <section>
          <h2><%= current_page.data.title || "news" %></h2>

          <div id="news">
          </div>

        </section>
        <hr class="major" />

      </div>
    </section>

    <script type="text/javascript" src="menu/menu.js"></script>
    <script type="text/javascript" src="js/translate.js"></script>

    <%= yield %>

  </body>

</html>